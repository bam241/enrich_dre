<simulation>

    <schematype>flat</schematype>
    <control>
        <duration>180</duration>
        <startmonth>1</startmonth>
        <startyear>2000</startyear>
        <explicit_inventory>true</explicit_inventory>
    </control>

    <archetypes>
        <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
        <spec> <lib>cycamore</lib><name>Source</name> </spec>
        <spec> <lib>cycamore</lib><name>Sink</name> </spec>
        <spec> <lib>cycamore</lib><name>Storage</name> </spec>
        <spec> <lib>mbmore</lib><name>CascadeEnrich</name> </spec>
    </archetypes>

    <prototype>
        <name>nu_src</name>
        <config>
            <Source>
                <outcommod>c_nat_u_</outcommod>
                <outrecipe>r_nat_u</outrecipe>
                <throughput>1000.0</throughput>
            </Source>
        </config>
    </prototype>


    <prototype>
        <name>waste</name>
        <config>
            <Sink>
                <in_commods>
                    <val>c_depleted_u</val>
                </in_commods>
            </Sink>
        </config>
    </prototype>

    <prototype>
        <name>cascade_0</name>
        <config>
            <CascadeEnrich>
                <order_prefs>0</order_prefs>
                <feed_commod>c_nat_u</feed_commod>
                <feed_recipe>r_nat_u</feed_recipe>
                <product_commod>c_enrich_u_0</product_commod>
                <tails_commod>c_depleted_u</tails_commod>
                <max_feed_inventory>100000</max_feed_inventory>

                <design_product_assay>0.026</design_product_assay>
                <design_tails_assay>0.002</design_tails_assay>
                <design_feed_assay>0.0071</design_feed_assay>
                <design_feed_flow>1000</design_feed_flow>
                <max_centrifuges>169</max_centrifuges>

                <fix_ab>1</fix_ab>
                <centrifuge_velocity>350</centrifuge_velocity>
                <height>1.8</height>
                <diameter>0.105</diameter>
                <machine_feed>13</machine_feed>
            </CascadeEnrich>
        </config>
    </prototype>
    <prototype>
        <name>cascade_1</name>
